{
	"name": "involve-digital/webloader",
	"description": "Tool for loading or deploying CSS and JS files into web pages",
	"keywords": ["webloader", "css", "javascript", "assets", "nette"],
	"homepage": "http://addons.nette.org/cs/webloader",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Marek",
			"email": "mail@janmarek.net"
		}
	],
	"autoload": {
		"psr-0": {
			"WebLoader": ""
		}
	},
	"require": {
		"nette/application": "^3.0",
		"nette/di": "^3.0",
		"nette/utils": "^4.0"
	},
	"suggest": {
		"oyejorge/less.php": "LESS compiler written in PHP.",
		"leafo/scssphp": "SCSS compiler written in PHP.",
		"joseki/webloader-filters": "CSSMin & JSMin filters written in PHP.",
		"coffeescript/coffeescript": "CoffeeScript compiler written in PHP."
	},
	"require-dev": {
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.0",
		"nette/component-model": "^3.0",
		"nette/database": "^3.0",
		"nette/deprecated": "~2.2@dev",
		"nette/finder": "^2.6 || ^3.0",
		"nette/forms": "^3.0",
		"nette/http": "^3.0",
		"nette/mail": "^3.0",
		"nette/neon": "^3.0",
		"nette/php-generator": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/safe-stream": "^3.0",
		"nette/security": "^3.0",
		"nette/tokenizer": "^3.0",
		"latte/latte": "^2.7 || ^3.0",
		"tracy/tracy": "^2.6",

		"oyejorge/less.php": "~1.5",
		"leafo/scssphp": "~0.1",
		"coffeescript/coffeescript": "1.3.*",

		"mockery/mockery": "0.7.*",
		"phpunit/phpunit": "3.7.*",
		"jakub-onderka/php-parallel-lint": "~0.7"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.4-dev"
		}
	}
}
